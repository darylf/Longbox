    "prettier:save": "prettier --config config/prettier.config.js '*/**/*.{js,ts,tsx}' --write",
    "start:dev": "webpack serve --host 0.0.0.0 --mode development  --config config/webpack.config.js",
    "test": "jest --config config/jest.config.js",
    "test:watch": "jest  --config config/jest.config.js --watchAll"
