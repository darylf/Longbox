{
  "compilerOptions": {
    "target": "es2019",
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "jsx": "react"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
