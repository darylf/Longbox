{
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "jsx": "react",
    "module": "esnext",
    "sourceMap": true,
    "target": "es2019",
    "strict": true,
    // "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
